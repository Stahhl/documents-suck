{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "templateRoute": {
        "ClusterId": "template",
        "Match": {
          "Path": "/template/{id}"
        }
      },
      "swaggerRoute": {
        "ClusterId": "template",
        "Match": {
          "Path": "/docs/{**catch-all}"
        }
      },
      "documentRoute": {
        "ClusterId": "document",
        "Match": {
          "Path": "/template/{id}/{ext}"
        },
        "Transforms": [
          {
            "PathPattern": "/document/{id}/{ext}"
          }
        ]
      }
    },
    "Clusters": {
      "template": {
        "Destinations": {
          "destinationA": {
            "Address": "http://127.0.0.1:8000"
          }
        }
      },
      "document": {
        "Destinations": {
          "destinationB": {
            "Address": "http://localhost:5099"
          }
        }
      }
    }
  }
}
